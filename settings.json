{
    "name"          : "gws_sim",
    "author"        : "Gencovery",
    "author_email"  : "admin@gencovery.com",
    "description"   : "Gencovery brick for dynamical system simulation",
    "version"       : "0.5.0-beta.3",
    "variables": {
        "gws_ode:testdata_dir": "${CURRENT_DIR}/tests/testdata"
    },
    "environment": {
        "pip": [
            {
                "source": "https://pypi.python.org/simple",
                "packages": [
                    {"name": "numba", "version": "0.56.4"},
                    {"name": "pytensor", "version": "2.10.1"},
                    {"name": "pymc", "version": "5.2.0"},
                    {"name": "arviz", "version": "0.15.1"}
                ]
            },
            {
                "source": "https://$GWS_PIP_PACKAGE_TOKEN@gitlab.com/api/.../packages/pypi",
                "packages": []
            }
        ],
        "git": [
            {
                "source": "https://$GWS_GIT_LOGIN:$GWS_GIT_PWD@gitlab.com/gencovery/core",
                "packages":[
                    {"name": "gws_core", "version": "0.5.0-beta.3", "is_brick": true}
                ]
            }
        ],
        "variables": {
            "GWS_GIT_LOGIN": "",
            "GWS_GIT_PWD": "",
            "GWS_PIP_PACKAGE_TOKEN": ""
        }
    }
}